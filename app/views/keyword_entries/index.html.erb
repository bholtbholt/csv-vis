<p id="notice"><%= notice %></p>

<h1>Listing Keyword Entries</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Google</th>
      <th>Google base rank</th>
      <th>Yahoo</th>
      <th>Bing</th>
      <th>Global monthly searches</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @keyword_entries.each do |keyword| %>
      <tr>
        <td><%= keyword.date %></td>
        <td><%= keyword.google %></td>
        <td><%= keyword.google_base_rank %></td>
        <td><%= keyword.yahoo %></td>
        <td><%= keyword.bing %></td>
        <td><%= keyword.global_monthly_searches %></td>
        <td><%= link_to 'Show', keyword %></td>
        <td><%= link_to 'Edit', edit_keyword_path(keyword) %></td>
        <td><%= link_to 'Destroy', keyword, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Keyword entry', new_keyword_entry_path %>
