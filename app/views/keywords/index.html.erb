<p id="notice"><%= notice %></p>
<%= form_tag upload_csv_keywords_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag :upload_csv %>
<% end %>
<h1>Listing Keywords</h1>

<table>
  <thead>
    <tr>
      <th>Site</th>
      <th>Keywords</th>
      <th>Date</th>
      <th>Google</th>
      <th>Google base rank</th>
      <th>Yahoo</th>
      <th>Bing</th>
      <th>Global monthly searches</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @keywords.each do |keyword| %>
      <tr>
        <td><%= keyword.site %></td>
        <td><%= keyword.keywords %></td>
        <td><%= keyword.date %></td>
        <td><%= keyword.google %></td>
        <td><%= keyword.google_base_rank %></td>
        <td><%= keyword.yahoo %></td>
        <td><%= keyword.bing %></td>
        <td><%= keyword.global_monthly_searches %></td>
        <td><%= link_to 'Show', keyword %></td>
        <td><%= link_to 'Edit', edit_keyword_path(keyword) %></td>
        <td><%= link_to 'Destroy', keyword, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Keyword', new_keyword_path %>
